<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_faktor_12">

    <include
        android:id="@+id/ucWMZPart"
        layout="@layout/list_item_nutzertodos_messgeraet_art_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"/>
    <include
        android:id="@+id/ucHKVPart"
        layout="@layout/list_item_nutzertodos_messgeraet_art_content"
        android:layout_toEndOf="@id/ucWMZPart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <include
        android:id="@+id/ucWWZPart"
        layout="@layout/list_item_nutzertodos_messgeraet_art_content"
        android:layout_below="@id/ucWMZPart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/ucKWZPart"
        layout="@layout/list_item_nutzertodos_messgeraet_art_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ucHKVPart"
        android:layout_toEndOf="@id/ucWWZPart" />
</RelativeLayout>